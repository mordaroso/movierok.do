<Addin 
	id="movierok" 
	namespace="Do"
	version="0.2"
	name="movierok.org Movie Search" 
	description="Searches and playes your movierok movies."
	author="movierok team"
>
 
	<Runtime>
		<Import assembly="Movierok.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
		<Extension path="/Do/Action">
		<Action type="Movierok.MovierokAction"/>
	</Extension>
	<Extension path="/Do/ItemSource">
		<ItemSource type="Movierok.MovierokItemSource" />
	</Extension>

</Addin>
